


	<h1> Enter Patient Details </h1>


	<%= form_for @p , url:{action: :create} do |f| %>
		<% if @p.errors.any? %>
			<% @p.errors.full_messages.each do |error| %>
				<%= error %></br>
	    	<% end %>
	 	<% end %>
		
		<table class="table table-hover">
			<%= render partial:'form',locals:{f:f} %>

		</table>
		
	<% end %>

 

